@model prjTeam2_Final.Models.tN5

@{
    ViewBag.Title = "Create";
}

<h2>新增單字</h2>

@using (Html.BeginForm()) 
{
    
    <div class="form-horizontal">   
        <hr />
        <div class="form-group">
            <label class="control-label col-md-2">日文:</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.日文, new { htmlAttributes = new { @class = "form-control", required = "required" } })
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">中文:</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.中文, new { htmlAttributes = new { @class = "form-control", required = "required" } })
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">假名:</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.假名, new { htmlAttributes = new { @class = "form-control", required = "required" } })
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">種類:</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.種類, new { htmlAttributes = new { @class = "form-control",required="required" } })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="新增單字" class="btn btn-primary" />
                @Html.ActionLink("回單字表", "Index","WordList","",new { @class= "btn btn-success" })
            </div>
        </div>
    </div>
}
